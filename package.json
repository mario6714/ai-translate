{
  "name": "ai-translate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && npm run build:css && vite build",
    "preview": "vite preview",
    "start": "python webview\\pywebview\\start.py",
    "pack": "pyinstaller webview\\pywebview\\start.py --onefile --noconsole --add-data \"dist/*.html;dist\" --add-data \"dist/*.ico;dist\" --add-data \"dist/assets/*;dist/assets\" --icon=dist/favicon.ico",
    "css:dev": "tailwindcss -i src/input.css -o output/output.css --watch",
    "build:css": "tailwindcss -i src/input.css -o output/output.css"
  },
  "dependencies": {
    "@google/generative-ai": "^0.9.0",
    "@solidjs/router": "^0.13.2",
    "buffer": "^6.0.3",
    "exceljs": "^4.4.0",
    "groq-sdk": "^0.3.2",
    "solid-js": "^1.8.15"
  },
  "devDependencies": {
    "@types/exceljs": "^1.3.0",
    "@types/node": "^20.12.7",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-solid": "^2.10.2"
  }
}
